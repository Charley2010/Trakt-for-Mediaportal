<extension_settings>
	<disable_entry>
		<setting entryname="plugins" name="Extensions" displayname="Plugin Enabled" defaultvalue="yes" type="string" listvalues="yes|no" displaylistvalues="Yes|No"/> 
	</disable_entry>
	<settings section="#Trakt.Translation.Plugin.Label">
    <setting entryname="plugins" name="Trakt" displayname="#Trakt.Translation.SettingPluginEnabledName.Label" defaultvalue="yes" type="string" listvalues="yes|no" displaylistvalues="#Trakt.Translation.Yes.Label|#Trakt.Translation.No.Label" description="#Trakt.Translation.SettingPluginEnabledDescription.Label" configfile="MediaPortal.xml" /> 
    <setting entryname="home" name="Trakt" displayname="#Trakt.Translation.SettingListedHomeName.Label" defaultvalue="yes" type="string" listvalues="yes|no" displaylistvalues="#Trakt.Translation.Yes.Label|#Trakt.Translation.No.Label" description="#Trakt.Translation.SettingListedHomeDescription.Label" configfile="MediaPortal.xml" /> 
    <setting entryname="myplugins" name="Trakt" displayname="#Trakt.Translation.SettingListedPluginsName.Label" defaultvalue="no" type="string" listvalues="yes|no" displaylistvalues="#Trakt.Translation.Yes.Label|#Trakt.Translation.No.Label" description="#Trakt.Translation.SettingListedPluginsDescription.Label" configfile="MediaPortal.xml" />
    <setting entryname="Trakt" name="SyncTimerLength" displayname="#Trakt.Translation.SettingSyncTimerName.Label" defaultvalue="86400000" type="string" listvalues="3600000|21600000|432000000|86400000" displaylistvalues="1|6|12|24" description="#Trakt.Translation.SettingSyncTimerDescription.Label" configfile="MediaPortal.xml" />
    <setting entryname="Trakt" name="WebRequestCacheMinutes" displayname="#Trakt.Translation.SettingWebRequestCacheName.Label" defaultvalue="15" type="string" listvalues="1|5|10|15|30|60|120|360|720|1440" displaylistvalues="1|5|10|15|30|60|120|360|720|1440" description="#Trakt.Translation.SettingWebRequestCacheDescription.Label" configfile="MediaPortal.xml" />
    <setting entryname="Trakt" name="WebRequestTimeout" displayname="#Trakt.Translation.SettingWebRequestTimeoutName.Label" defaultvalue="15000" type="string" listvalues="15000|30000|45000|60000|90000" displaylistvalues="15|30|45|60|90" description="#Trakt.Translation.SettingWebRequestTimeoutDescription.Label" configfile="MediaPortal.xml" />
  </settings>
</extension_settings>